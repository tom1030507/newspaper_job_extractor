# Gemini API 密鑰
# 請到 https://aistudio.google.com/app/apikey 獲取您的 API 密鑰
GEMINI_API_KEY="YOUR_GEMINI_API_KEY"

# Flask 配置
FLASK_ENV=production
FLASK_DEBUG=False

# 應用程式配置
MAX_CONTENT_LENGTH=16777216
UPLOAD_FOLDER=uploads
RESULTS_FOLDER=results

# 可選配置
# FLASK_HOST=0.0.0.0
# FLASK_PORT=8080 
GOOGLE_APPS_SCRIPT_URL="YOUR_ACTUAL_GOOGLE_APPS_SCRIPT_URL_HERE"